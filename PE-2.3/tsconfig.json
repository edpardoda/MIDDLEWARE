{
  "compilerOptions": {
    "module": "nodenext",
    "target": "ESNext",
    "moduleResolution": "nodenext",
    "lib": [
      "ESNext"
    ],
    "outDir": "./dist",
    "rootDir":"./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}